export const snapshot = {};

snapshot[`user facing logs > given commit that does not trigger a release, expect logs to communicate this clearly 1`] = `
[
  "chore: does not trigger a release (abc123) => does not indicate a release.",
]
`;

snapshot[`user facing logs > given super long commit messages, expect logs to not be super long 1`] = `
[
  "feat: aaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa... (abc123) => indicates a minor release.",
]
`;

snapshot[`user facing logs > given commit message multiple lines long, expect logs to not be super long 1`] = `
[
  "feat: a (abc123) => indicates a minor release.",
]
`;

snapshot[`user facing logs > given many commits, expect logs to read them all nicely 1`] = `
[
  "feat: add new feature (abc123) => indicates a minor release.",
  "fix: resolve issue (abc123) => indicates a patch release.",
  "chore: update dependencies (abc123) => does not indicate a release.",
]
`;
